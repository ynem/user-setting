# User specific aliases and functions
alias j='ls -la'
alias l='less -iMS'
alias jl='ls -la | less -iMS'
alias h='history'
alias hl='history | less -iMS'
alias g='grep -iE --color=auto'
alias gr='grep -irE --color=auto'
alias gst='git status'
alias gl='git log --decorate --pretty=oneline --graph --all --abbrev-commit'
alias gc='git commit -am'
alias sst='svn status'

stty stop undef

